<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="line">

    <!--显示一条虚线，width为线条的高度，-->
    <!--dashWidth为破折线的宽度，-->
    <!--dashGap为破折线之间的空隙的宽度，-->
    <!--当dashGap=0时，就是实线-->
    <stroke
        android:width="5px"
        android:color="#FCA142"
        android:dashWidth="10px"
        android:dashGap="10px" />
</shape>